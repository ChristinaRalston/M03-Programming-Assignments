things = ["mozzarella", "cinderella", "salmonella"]

things[1] = things[1].capitalize()

things[0] = things[0].upper()

del things [2]
#it took me a long time to realize why this worked and that cinderells is in the 1 spot
print(things)



def good():
    return ['Harry', 'Ron', 'Hermione']
    

def get_odds ():
    for numbers in range (1, 10, 2):
        yield numbers
        
for i, numbers in enumerate(get_odds()):
    if i == 2:
        print (numbers)
        break